{
	"name" : "bobdenotter/rssaggregator",
	"description" : "RSS Feed reader and aggregator extension for Bolt",
	"type" : "bolt-extension",
	"keywords" : [
		"RSS", "Feed", "Feedreader", "aggregator", "aggregation"
	],
    "require": {
        "bolt/bolt": "^3.0",
		"fguillot/picofeed": "@stable"
    },
	"license" : "MIT",
	"authors" : [{
			"name" : "Bob den Otter",
			"email" : "bob@twokings.nl"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Bolt\\Extension\\Bobdenotter\\RSSAggregator\\" : "src"
		}
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\Bobdenotter\\RSSAggregator\\RSSAggregatorExtension"
    }
}
